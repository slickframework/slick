{
    "name": "slick/cache",
    "description": "Cache component for Slick Framework",
    "license": "MIT",
    "keywords": ["slick", "cache", "memcached"],
    "autoload": {
        "psr-0": {
            "Slick\\Cache\\": ""
        }
    },
    "target-dir": "Slick/Cache",
    "require": {
        "slick/common": "self.version",
        "slick/filesystem": "self.version"
    },
    "suggest": {
        "ext-memcache": "For a better performance cache"
    }
}